<h2>Carrito: {{cid}}</h2>
<section >
    <table id="tablaCarrito">
        <thead>
            <tr>
                <th>Producto</th>
                <th>Cantidad</th>
            </tr>
        </thead>
        <tbody>
            {{#each theCart}}
            <tr>
                <td>{{this.productId.title}}</td>
                <td>{{this.quantity}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</section>